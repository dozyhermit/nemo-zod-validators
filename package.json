{
  "name": "@dozyhermit/nemo-zod-validators",
  "version": "1.1.6",
  "description": "A collection of middleware validators made with zod and @rescale/nemo for Next.js",
  "license": "MIT",
  "author": "dozyhermit (https://github.com/dozyhermit)",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "directories": {
    "lib": "lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dozyhermit/nemo-zod-validators.git"
  },
  "homepage": "https://github.com/dozyhermit/nemo-zod-validators#readme",
  "bugs": {
    "url": "https://github.com/dozyhermit/nemo-zod-validators/issues"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "keywords": [
    "zod",
    "@rescale/nemo",
    "Next.js"
  ],
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "clean": "rm -rf ./lib/",
    "lint": "eslint",
    "types:check": "tsc --noEmit",
    "prettier:check": "prettier . --check",
    "format:write": "prettier . --write",
    "format:check": "npm run types:check && npm run lint && npm run prettier:check",
    "test": "jest --coverage"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.8",
    "@eslint/eslintrc": "^3",
    "typescript-eslint": "^8.25.0",
    "eslint": "^9.21.0",
    "jest": "^29.7.0",
    "prettier": "^3.5.2",
    "ts-jest": "^29.2.6",
    "typescript": "^5"
  },
  "dependencies": {
    "@rescale/nemo": "^1.4.0",
    "next": "^15.2.0",
    "zod": "^3.24.2"
  }
}
